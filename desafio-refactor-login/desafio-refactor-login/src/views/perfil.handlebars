<header>
    <div id="PreHeader">
        {{> submenu}}
    </div>
    <div id="MainHeader">
        <div id="LogoHeader">Lista del Sol</div>
        <div id="MenuHeader">{{>menu}}</div>
    </div>
</header>
<main>
    <div id="container">
        <h1>Perfíl de Usuario</h1>
        <div id="perfil">
            <h2>Datos de Usuario</h2>
        
            <div class="espaciado">
                {{#if login.profile._json.avatar_url}}<p class="avatarBig"><img src="{{login.profile._json.avatar_url}}" alt="GitHub Avatar"></p>{{/if}}
                {{#if login.profile._json.bio}}<p class="bio">{{login.profile._json.bio}}</p>{{/if}}
                <p><strong>Nombre:</strong> {{login.first_name}} {{login.last_name}}</p>
                {{#if age}}<p><strong>Edad:</strong> {{login.age}}</p>{{/if}}
                <p><strong>Correo Electrónico:</strong> {{login.email}}</p>
            </div>
        </div>
    </div>
</main>
<footer>
    <ul>
        <li><a href="/home" >Página de Inicio</a></li>
    </ul>
    
</footer>